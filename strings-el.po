#, c-format
msgctxt "imageselect_error_folder_cannot_delete"
msgid "Cannot delete folder '%1$s'."
msgstr "Δεν μπορεί να διαγραφεί ο φάκελος '%1$s'."

#, c-format
msgctxt "imageselect_error_cannot_write_data_bundle"
msgid "Cannot write data for image '%1$s'."
msgstr "Δεν μπορούν να αποθηκευτούν τα δεδομένα της εικόνας '%1$s'."

#, c-format
msgctxt "imageselect_error_cannot_write_data_bundle_detailed"
